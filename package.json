    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^3.11.1",
    "webpack-dev-server": "^2.11.4"
  }
